{
  "name": "next-saas-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:fast": "next build",
    "build:production": "npm run lint:ts && npm run build",
    "build:vercel": "next build",
    "start": "next start",
    "lint": "next lint",
    "email": "email dev",
    "lint:ts": "tsc --noEmit",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhook/stripe",
    "test": "node scripts/run-tests.js",
    "test:ui": "node scripts/run-tests.js ui",
    "test:debug": "node scripts/run-tests.js debug",
    "test:headed": "node scripts/run-tests.js headed",
    "test:ai": "node scripts/run-tests.js ai",
    "test:browsers": "node scripts/run-tests.js browsers",
    "test:mobile": "node scripts/run-tests.js mobile",
    "test:install": "npm install --save-dev @playwright/test && npx playwright install",
    "security:validate": "node scripts/validate-security.js",
    "security:audit": "node scripts/validate-security.js",
    "staging:prepare": "npm run security:validate && npm run test && npm run build",
    "staging:deploy": "vercel --prebuilt",
    "production:prepare": "npm run staging:prepare && npm run test:browsers",
    "production:deploy": "vercel --prod --prebuilt",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "ci:validate": "npm run lint:ts && npm run security:validate && npm run build",
    "clean": "rm -rf .next node_modules/.cache",
    "reinstall": "npm run clean && npm install",
    "dev:windows": "set NODE_OPTIONS=--max-old-space-size=4096 && next dev"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.7.4",
    "@hookform/resolvers": "^5.0.1",
    "@next/third-parties": "^15.1.7",
    "@openpanel/nextjs": "^1.0.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-email/components": "^0.0.33",
    "@react-email/render": "^1.0.5",
    "@react-pdf/renderer": "^4.3.0",
    "@stripe/stripe-js": "^5.6.0",
    "@supabase/supabase-js": "^2.48.1",
    "@xenova/transformers": "^2.17.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.15.0",
    "jose": "^5.10.0",
    "lucide-react": "^0.475.0",
    "ml-matrix": "^6.12.1",
    "motion": "^12.15.0",
    "next": "15.1.7",
    "next-auth": "5.0.0-beta.25",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.0",
    "performance-now": "^2.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-email": "^3.0.7",
    "react-hook-form": "^7.56.4",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "redis": "^5.1.1",
    "stripe": "17.6.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.45"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
